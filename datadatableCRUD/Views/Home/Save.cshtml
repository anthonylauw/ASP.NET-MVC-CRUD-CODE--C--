@model datadatableCRUD.Models.employee
<h2>Save</h2>
@using (Html.BeginForm("save", "home", FormMethod.Post, new { id = "popupForm" }))
{
    if (Model != null && Model.EmployeeId > 0)
    {
        @Html.HiddenFor(a => a.EmployeeId)
    }

    <div class="form-group">
        <label>First Name</label>
        @Html.TextBoxFor(a => a.FirstName, new { @class = "form-control" })
        @Html.ValidationMessageFor(a => a.FirstName)
    </div>
    <div class="form-group">
        <label>Last Name</label>
        @Html.TextBoxFor(a => a.LastName, new { @class = "form-control" })
        @Html.ValidationMessageFor(a => a.LastName)
    </div>
    <div class="form-group">
        <label>Email</label>
        @Html.TextBoxFor(a => a.EmailID, new { @class = "form-control" })
        @Html.ValidationMessageFor(a => a.EmailID)
    </div>
    <div class="form-group">
        <label>City</label>
        @Html.TextBoxFor(a => a.City, new { @class = "form-control" })
        @Html.ValidationMessageFor(a => a.City)
    </div>
    <div class="form-group">
        <label>Country</label>
        @Html.TextBoxFor(a => a.Country, new { @class = "form-control" })
        @Html.ValidationMessageFor(a => a.Country)
    </div>
    <div class="form-group">
        <label>Age</label>
        @Html.TextBoxFor(a => a.Age, new { @class = "form-control" })
        @Html.ValidationMessageFor(a => a.Age)
    </div>
    <div class="form-group">
        <label>Income</label>
        @Html.TextBoxFor(a => a.Income, new { @class = "form-control" })
        @Html.ValidationMessageFor(a => a.Income)
    </div>
    <div class="form-group">
        <label>Annual Income</label>
        @Html.TextBoxFor(a => a.Annual_income, new { @class = "form-control" })
        @Html.ValidationMessageFor(a => a.Income)
    </div>


    <div>
        <input type="submit" value="Save" />
    </div>
}
<script>
    $(function () {

        $("#Income").keyup(function (e) {

            var q = $("#Income").val();
            var result = "";

            if (q !== "" && $.isNumeric(q)) {
                result = parseFloat(q) * 12;
            }
            $("#Annual_income").val(result);

        });

    });


</script>